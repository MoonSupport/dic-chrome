{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": { 
    "scripts": [
    "background.js"
  ]
},
"options_page": "options.html",
"content_scripts": [{
  "matches": [ "<all_urls>" ],
  "all_frames": true,
  "js": [
    "contentscript.js"
  ]
}],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    },
      "copy-translation-to-clipboard": {
        "description": "Copy translation to clipboard"
    }
  },
  "permissions": [ "https://clients5.google.com/*", "tabs" ],
  "web_accessible_resources": [
    "options.html"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://clients5.google.com; object-src 'self'"

}
